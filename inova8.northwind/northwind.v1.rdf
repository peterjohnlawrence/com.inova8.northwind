<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sp="http://spinrdf.org/sp#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:smf="http://topbraid.org/sparqlmotionfunctions#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns:spl="http://spinrdf.org/spl#"
    xmlns="http://northwind.com/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://northwind.com/">
  <owl:Ontology rdf:about="">
    <owl:versionInfo>v1</owl:versionInfo>
    <owl:priorVersion rdf:resource="http://northwind.com/v0"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:versionIRI rdf:resource="http://northwind.com/v1"/>
    <owl:imports rdf:resource="http://northwind.com/data/"/>
    <owl:imports rdf:resource="http://northwind.com/model/"/>
    <owl:imports rdf:resource="http://northwind.com/operations/"/>
  </owl:Ontology>
  <spin:MagicProperty rdf:about="http://northwind.com/modelDefinition">
    <spin:body>
      <sp:Select>
        <sp:text>SELECT ?modelDefinition
WHERE {
    {
        SELECT ?arg1 ?arg2 ?arg3 ((CONCAT("&lt;b&gt;", ?classLabel, "&lt;/b&gt;", ?nextClassProperties, "&lt;blockquote&gt;that ", ?classDefinition, "&lt;/blockquote&gt;")) AS ?modelDefinition)
        WHERE {
           # BIND (model:Order AS ?arg1) .  BIND (4 AS ?arg2) . BIND (false AS ?arg3) .
            ( ?arg1 ?arg2 ?arg3 "") :classDefinition (?classDefinition "") .
            ( ?arg1 ?arg3 ) :classProperties ?nextClassProperties .
            ?arg1 rdfs:label ?classLabel .
        }
    } .
}</sp:text>
        <sp:resultVariables rdf:parseType="Collection">
          <rdf:Description>
            <sp:varName>modelDefinition</sp:varName>
          </rdf:Description>
        </sp:resultVariables>
        <sp:where rdf:parseType="Collection">
          <sp:SubQuery>
            <sp:query>
              <sp:Select>
                <sp:resultVariables rdf:parseType="Collection">
                  <rdf:Description rdf:about="http://spinrdf.org/spin#_arg1"/>
                  <rdf:Description rdf:about="http://spinrdf.org/spin#_arg2"/>
                  <rdf:Description rdf:about="http://spinrdf.org/spin#_arg3"/>
                  <rdf:Description>
                    <sp:expression>
                      <sp:concat>
                        <sp:arg7>&lt;/blockquote&gt;</sp:arg7>
                        <sp:arg6 rdf:parseType="Resource">
                          <sp:varName>classDefinition</sp:varName>
                        </sp:arg6>
                        <sp:arg5>&lt;blockquote&gt;that </sp:arg5>
                        <sp:arg4 rdf:parseType="Resource">
                          <sp:varName>nextClassProperties</sp:varName>
                        </sp:arg4>
                        <sp:arg3>&lt;/b&gt;</sp:arg3>
                        <sp:arg2 rdf:parseType="Resource">
                          <sp:varName>classLabel</sp:varName>
                        </sp:arg2>
                        <sp:arg1>&lt;b&gt;</sp:arg1>
                      </sp:concat>
                    </sp:expression>
                    <sp:varName>modelDefinition</sp:varName>
                  </rdf:Description>
                </sp:resultVariables>
                <sp:where rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:object rdf:resource="http://spinrdf.org/spin#_arg1"/>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#first"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?0</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>?1</sp:varName>
                    </sp:object>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#rest"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?0</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:resource="http://spinrdf.org/spin#_arg2"/>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#first"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?1</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>?2</sp:varName>
                    </sp:object>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#rest"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?1</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:resource="http://spinrdf.org/spin#_arg3"/>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#first"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?2</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>?3</sp:varName>
                    </sp:object>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#rest"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?2</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object></sp:object>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#first"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?3</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#rest"/>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName>?3</sp:varName>
                    </sp:subject>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName>?4</sp:varName>
                    </sp:object>
                    <sp:predicate>
                      <spin:MagicProperty rdf:about="http://northwind.com/classDefinition">
                        <spin:body>
                          <sp:Select>
                            <sp:text>SELECT ?classDefinition ?priorPath
WHERE {
    {
        SELECT ?arg1 ?arg2  ((GROUP_CONCAT( ?definition; SEPARATOR='&lt;br/&gt;and that ')) AS ?classDefinition)  ((GROUP_CONCAT( ?pastPath; SEPARATOR='\t')) AS ?priorPath) 
        WHERE {
            ?arg1 a ?class . FILTER( ?class in (rdfs:Class, owl:Class ))
#,  odata4sparql:Operation))  .
            ?arg1 rdfs:label ?classLabel .
            ?objectProperty a owl:ObjectProperty .
            {
                        ?objectProperty rdfs:domain ?arg1 .
                        ?objectProperty rdfs:label ?objectPropertyLabel .
                        ?objectProperty rdfs:range ?nextClass .
                        ?nextClass rdfs:label ?nextClassLabel .   BIND(?objectProperty as ?property)
            }UNION{
				       ?objectProperty  owl:inverseOf ?inverseObjectProperty .
                        ?objectProperty rdfs:domain  ?nextClass.
                        ?inverseObjectProperty rdfs:label ?objectPropertyLabel .
                        ?objectProperty rdfs:range ?arg1 . 
                        ?nextClass rdfs:label ?nextClassLabel .    BIND(?inverseObjectProperty as ?property)
            }UNION{
				       ?inverseObjectProperty  owl:inverseOf ?objectProperty .
                        ?objectProperty rdfs:domain  ?nextClass.
                        ?inverseObjectProperty rdfs:label ?objectPropertyLabel .
                        ?objectProperty rdfs:range  ?arg1 . 
                        ?nextClass rdfs:label ?nextClassLabel .    BIND(?inverseObjectProperty as ?property)
            }&#xD;
#Stop from going too deep
            BIND(?arg2 -1 as ?span) FILTER(?span&gt;0).  
            ?nextClass a ?nextClassClass. FILTER( ?nextClassClass in (rdfs:Class, owl:Class ))
#,  odata4sparql:Operation))  . &#xD;
#Do not process an already processed arc (objectProperty)          
            BIND(CONCAT(?arg4,"\t",?objectPropertyLabel) as ?forwardPath) FILTER( !CONTAINS(?arg4, ?objectPropertyLabel ))   
            (?nextClass ?span ?arg3 ?forwardPath ) :classDefinition (?nextClassDefinition  ?nextPath).            &#xD;
#Do not include if arc (objectProperty) appears already    &#xD;
            FILTER(  !CONTAINS(?nextPath, ?objectPropertyLabel )) BIND(CONCAT( ?objectPropertyLabel, IF(?nextPath="","",CONCAT("\t",?nextPath))) as ?pastPath)
 		    (?nextClass ?arg3) :classProperties ?nextClassProperties .
            BIND (CONCAT("&lt;u&gt;",?objectPropertyLabel , "&lt;/u&gt; &lt;b&gt;", ?nextClassLabel, "&lt;/b&gt;",  ?nextClassProperties, IF ((?nextClassDefinition!=""), CONCAT("&lt;br/&gt;&lt;blockquote&gt;that ",  ?nextClassDefinition, "&lt;/blockquote&gt;"), "")  ) as ?definition)
        }
        GROUP BY ?arg1 ?arg2
    } .
}</sp:text>                 <sp:resultVariables rdf:parseType="Collection">
                              <rdf:Description>
                                <sp:varName>classDefinition</sp:varName>
                              </rdf:Description>
                              <rdf:Description>
                                <sp:varName>priorPath</sp:varName>
                              </rdf:Description>
                            </sp:resultVariables>
                            <sp:where rdf:parseType="Collection">
                              <sp:SubQuery>
                                <sp:query>
                                  <sp:Select>
                                    <sp:groupBy rdf:parseType="Collection">
                                      <rdf:Description rdf:about="http://spinrdf.org/spin#_arg1"/>
                                      <rdf:Description rdf:about="http://spinrdf.org/spin#_arg2"/>
                                    </sp:groupBy>
                                    <sp:resultVariables rdf:parseType="Collection">
                                      <rdf:Description rdf:about="http://spinrdf.org/spin#_arg1"/>
                                      <rdf:Description rdf:about="http://spinrdf.org/spin#_arg2"/>
                                      <rdf:Description>
                                        <sp:expression>
                                          <sp:GroupConcat>
                                            <sp:separator>&lt;br/&gt;and that </sp:separator>
                                            <sp:expression rdf:parseType="Resource">
                                              <sp:varName>definition</sp:varName>
                                            </sp:expression>
                                          </sp:GroupConcat>
                                        </sp:expression>
                                        <sp:varName>classDefinition</sp:varName>
                                      </rdf:Description>
                                      <rdf:Description>
                                        <sp:expression>
                                          <sp:GroupConcat>
                                            <sp:separator>	</sp:separator>
                                            <sp:expression rdf:parseType="Resource">
                                              <sp:varName>pastPath</sp:varName>
                                            </sp:expression>
                                          </sp:GroupConcat>
                                        </sp:expression>
                                        <sp:varName>priorPath</sp:varName>
                                      </rdf:Description>
                                    </sp:resultVariables>
                                    <sp:where rdf:parseType="Collection">
                                      <rdf:Description>
                                        <sp:object rdf:parseType="Resource">
                                          <sp:varName>class</sp:varName>
                                        </sp:object>
                                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                                        <sp:subject rdf:resource="http://spinrdf.org/spin#_arg1"/>
                                      </rdf:Description>
                                      <sp:Filter>
                                        <sp:expression>
                                          <sp:in>
                                            <sp:arg3 rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                                            <sp:arg2 rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
                                            <sp:arg1 rdf:parseType="Resource">
                                              <sp:varName>class</sp:varName>
                                            </sp:arg1>
                                          </sp:in>
                                        </sp:expression>
                                      </sp:Filter>
                                      <rdf:Description>
                                        <sp:object rdf:parseType="Resource">
                                          <sp:varName>classLabel</sp:varName>
                                        </sp:object>
                                        <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                        <sp:subject rdf:resource="http://spinrdf.org/spin#_arg1"/>
                                      </rdf:Description>
                                      <rdf:Description>
                                        <sp:object rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
                                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                                        <sp:subject rdf:parseType="Resource">
                                          <sp:varName>objectProperty</sp:varName>
                                        </sp:subject>
                                      </rdf:Description>
                                      <sp:Union>
                                        <sp:elements rdf:parseType="Collection">
                                          <rdf:Description>
                                            <rdf:rest rdf:parseType="Collection">
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectPropertyLabel</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClass</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#range"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClassLabel</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClass</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <sp:Bind>
                                                <sp:expression rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:expression>
                                                <sp:variable rdf:parseType="Resource">
                                                  <sp:varName
                                                  >property</sp:varName>
                                                </sp:variable>
                                              </sp:Bind>
                                            </rdf:rest>
                                            <rdf:first rdf:parseType="Resource">
                                              <sp:object rdf:resource="http://spinrdf.org/spin#_arg1"/>
                                              <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
                                              <sp:subject rdf:parseType="Resource">
                                                <sp:varName>objectProperty</sp:varName>
                                              </sp:subject>
                                            </rdf:first>
                                          </rdf:Description>
                                          <rdf:Description>
                                            <rdf:rest rdf:parseType="Collection">
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClass</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectPropertyLabel</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >inverseObjectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:resource="http://spinrdf.org/spin#_arg1"/>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#range"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClassLabel</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClass</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <sp:Bind>
                                                <sp:expression rdf:parseType="Resource">
                                                  <sp:varName
                                                  >inverseObjectProperty</sp:varName>
                                                </sp:expression>
                                                <sp:variable rdf:parseType="Resource">
                                                  <sp:varName
                                                  >property</sp:varName>
                                                </sp:variable>
                                              </sp:Bind>
                                            </rdf:rest>
                                            <rdf:first rdf:parseType="Resource">
                                              <sp:object rdf:parseType="Resource">
                                                <sp:varName>inverseObjectProperty</sp:varName>
                                              </sp:object>
                                              <sp:predicate rdf:resource="http://www.w3.org/2002/07/owl#inverseOf"/>
                                              <sp:subject rdf:parseType="Resource">
                                                <sp:varName>objectProperty</sp:varName>
                                              </sp:subject>
                                            </rdf:first>
                                          </rdf:Description>
                                          <rdf:Description>
                                            <rdf:rest rdf:parseType="Collection">
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClass</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectPropertyLabel</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >inverseObjectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:resource="http://spinrdf.org/spin#_arg1"/>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#range"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectProperty</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <rdf:Description>
                                                <sp:object rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClassLabel</sp:varName>
                                                </sp:object>
                                                <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                                                <sp:subject rdf:parseType="Resource">
                                                  <sp:varName
                                                  >nextClass</sp:varName>
                                                </sp:subject>
                                              </rdf:Description>
                                              <sp:Bind>
                                                <sp:expression rdf:parseType="Resource">
                                                  <sp:varName
                                                  >inverseObjectProperty</sp:varName>
                                                </sp:expression>
                                                <sp:variable rdf:parseType="Resource">
                                                  <sp:varName
                                                  >property</sp:varName>
                                                </sp:variable>
                                              </sp:Bind>
                                            </rdf:rest>
                                            <rdf:first rdf:parseType="Resource">
                                              <sp:object rdf:parseType="Resource">
                                                <sp:varName>objectProperty</sp:varName>
                                              </sp:object>
                                              <sp:predicate rdf:resource="http://www.w3.org/2002/07/owl#inverseOf"/>
                                              <sp:subject rdf:parseType="Resource">
                                                <sp:varName>inverseObjectProperty</sp:varName>
                                              </sp:subject>
                                            </rdf:first>
                                          </rdf:Description>
                                        </sp:elements>
                                      </sp:Union>
                                      <sp:Bind>
                                        <sp:expression>
                                          <sp:sub>
                                            <sp:arg2 rdf:datatype=
                                            "http://www.w3.org/2001/XMLSchema#integer"
                                            >1</sp:arg2>
                                            <sp:arg1 rdf:resource="http://spinrdf.org/spin#_arg2"/>
                                          </sp:sub>
                                        </sp:expression>
                                        <sp:variable rdf:parseType="Resource">
                                          <sp:varName>span</sp:varName>
                                        </sp:variable>
                                      </sp:Bind>
                                      <sp:Filter>
                                        <sp:expression>
                                          <sp:gt>
                                            <sp:arg2 rdf:datatype=
                                            "http://www.w3.org/2001/XMLSchema#integer"
                                            >0</sp:arg2>
                                            <sp:arg1 rdf:parseType="Resource">
                                              <sp:varName>span</sp:varName>
                                            </sp:arg1>
                                          </sp:gt>
                                        </sp:expression>
                                      </sp:Filter>
                                      <rdf:Description>
                                        <sp:object rdf:parseType="Resource">
                                          <sp:varName>nextClassClass</sp:varName>
                                        </sp:object>
                                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                                        <sp:subject rdf:parseType="Resource">
                                          <sp:varName>nextClass</sp:varName>
                                        </sp:subject>
                                      </rdf:Description>
                                      <sp:Filter>
                                        <sp:expression>
                                          <sp:in>
                                            <sp:arg3 rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                                            <sp:arg2 rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
                                            <sp:arg1 rdf:parseType="Resource">
                                              <sp:varName>nextClassClass</sp:varName>
                                            </sp:arg1>
                                          </sp:in>
                                        </sp:expression>
                                      </sp:Filter>
                                      <sp:Bind>
                                        <sp:expression>
                                          <sp:concat>
                                            <sp:arg3 rdf:parseType="Resource">
                                              <sp:varName>objectPropertyLabel</sp:varName>
                                            </sp:arg3>
                                            <sp:arg2>	</sp:arg2>
                                            <sp:arg1 rdf:resource="http://spinrdf.org/spin#_arg4"/>
                                          </sp:concat>
                                        </sp:expression>
                                        <sp:variable rdf:parseType="Resource">
                                          <sp:varName>forwardPath</sp:varName>
                                        </sp:variable>
                                      </sp:Bind>
                                      <sp:Filter>
                                        <sp:expression>
                                          <sp:not>
                                            <sp:arg1>
                                              <sp:contains>
                                                <sp:arg2 rdf:parseType="Resource">
                                                  <sp:varName
                                                  >objectPropertyLabel</sp:varName>
                                                </sp:arg2>
                                                <sp:arg1 rdf:resource="http://spinrdf.org/spin#_arg4"/>
                                              </sp:contains>
                                            </sp:arg1>
                                          </sp:not>
                                        </sp:expression>
                                      </sp:Filter>
                                      <rdf:Description>
                                        <sp:object rdf:parseType="Resource">
                                          <sp:varName>nextClass</sp:varName>
                                        </sp:object>
                                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#first"/>
                                        <sp:subject rdf:parseType="Resource">
                                          <sp:varName>?0</sp:varName>
                                        </sp:subject>
                                      </rdf:Description>
                                      <rdf:Description>
                                        <sp:object rdf:parseType="Resource">
                                          <sp:varName>?1</sp:varName>
                                        </sp:object>
                                        <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#rest"/>
                                        <sp:subject rdf:parseType="Resource">
                                          <sp:varName>?0</sp:varName>
                                        </sp:subject>
                                      </rdf:Description>
                                      <rdf:Description>
                                        <sp:object rdf:parseType="Resource">
