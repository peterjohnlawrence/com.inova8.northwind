              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >orderdetail</sp:varName>
            </sp:subject>
            <sp:object rdf:resource="http://spinrdf.org/spin#_arg1"/>
          </rdf:Description>
          <rdf:Description>
            <sp:predicate rdf:resource="http://northwind.com/model/product"/>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >orderdetail</sp:varName>
            </sp:subject>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >prod</sp:varName>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:predicate rdf:resource="http://northwind.com/model/quantity"/>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >orderdetail</sp:varName>
            </sp:subject>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >quantity</sp:varName>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:predicate rdf:resource="http://northwind.com/model/unitPrice"/>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >orderdetail</sp:varName>
            </sp:subject>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >price</sp:varName>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:predicate rdf:resource="http://northwind.com/model/discount"/>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >orderdetail</sp:varName>
            </sp:subject>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >discount</sp:varName>
            </sp:object>
          </rdf:Description>
        </sp:where>
        <sp:resultVariables rdf:parseType="Collection">
          <rdf:Description>
            <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >order</sp:varName>
            <sp:expression rdf:resource="http://spinrdf.org/spin#_arg1"/>
          </rdf:Description>
          <rdf:Description>
            <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >value</sp:varName>
            <sp:expression>
              <sp:Sum>
                <sp:expression>
                  <sp:mul>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >quantity</sp:varName>
                    </sp:arg1>
                    <sp:arg2>
                      <sp:mul>
                        <sp:arg1 rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >price</sp:varName>
                        </sp:arg1>
                        <sp:arg2>
                          <sp:sub>
                            <sp:arg1 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                            >1</sp:arg1>
                            <sp:arg2 rdf:parseType="Resource">
                              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >discount</sp:varName>
                            </sp:arg2>
                          </sp:sub>
                        </sp:arg2>
                      </sp:mul>
                    </sp:arg2>
                  </sp:mul>
                </sp:expression>
              </sp:Sum>
            </sp:expression>
          </rdf:Description>
          <rdf:Description>
            <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >count</sp:varName>
            <sp:expression>
              <sp:Sum>
                <sp:expression rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >quantity</sp:varName>
                </sp:expression>
              </sp:Sum>
            </sp:expression>
          </rdf:Description>
        </sp:resultVariables>
        <sp:groupBy rdf:parseType="Collection">
          <rdf:Description rdf:about="http://spinrdf.org/spin#_arg1"/>
        </sp:groupBy>
        <sp:text rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >select  (?arg1 as ?order ) (sum(?quantity * ( ?price * (1-?discount) )) as ?value)  (sum(?quantity) as ?count)&#xD;
where{&#xD;
?orderdetail a &lt;http://northwind.com/OrderDetail&gt; .&#xD;
?orderdetail &lt;http://northwind.com/order&gt; ?arg1 .&#xD;
?orderdetail &lt;http://northwind.com/product&gt; ?prod.&#xD;
?orderdetail &lt;http://northwind.com/quantity&gt; ?quantity .&#xD;
?orderdetail &lt;http://northwind.com/unitPrice&gt; ?price .&#xD;
?orderdetail &lt;http://northwind.com/discount&gt; ?discount &#xD;
} group by ?arg1</sp:text>
      </sp:Select>
    </spin:body>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Order quantity</rdfs:label>
